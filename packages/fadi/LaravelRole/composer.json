            "homepage": "https://fadisarwat.dev"
        }
    ],
    "homepage": "https://github.com/Fadi-S/laravelrole",
    "keywords": ["Laravel", "LaravelRole"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Fadi\\LaravelRole\\": "src/",
            "Fadi\\LaravelRole\\Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fadi\\LaravelRole\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fadi\\LaravelRole\\LaravelRoleServiceProvider"
            ],
            "aliases": {
                "LaravelRole": "Fadi\\LaravelRole\\Facades\\LaravelRole"
            }
        }
    }
}
